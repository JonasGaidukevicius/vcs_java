package lt.vcs.andrius.referencevsvalue;

public class Taskas {
	
		
	public static int getSkaicius() {
		System.out.println("Skaicius: " + skaicius);
		return skaicius;
	}
	
	public static int skaicius;

	public int x;
	public int y;

	public Taskas(int x, int y) {
		super();
		this.x = x;
		this.y = y;
		skaicius++;
	}

	@Override
	public String toString() {
		return "Taskas --- [x=" + x + ", y=" + y + "]";
	}

}
